@{

    string type = (string)Session["usserType"];

    if (type == "Administración")
    {
        Layout = "~/Views/Shared/_Layout_Admin.cshtml";
    }
    else if (type == "Jefatura")
    {
        Layout = "~/Views/Shared/_Layout_Jefatura.cshtml";
    }
    else if (type == "Estándar")
    {
        Layout = "~/Views/Shared/_Layout_Empleado.cshtml";
    }
    
}


